
<!-- Search bar -->


<!-- List of the events -->
<div class="wrapper">
  <div class="container">
    <div class="row">
      <div class="col-xs-12">
        <% unless @events.nil? %>
        <ul class="list-unstyled">
          <%  @events.each do |event| %>
            <li>
              <h2><%= link_to event.title, event_path(event) %></h2>
            </li>
          <% end %>
        </ul>
        <% end %>
      </div>
    </div>
  </div>
</div>
